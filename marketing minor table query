drop table marketing_minor;
create table marketing_minor
	(course_number varchar(6) NOT NULL,
	course_title varchar(100) NOT NULL,
	course_description varchar (255),
	requirement bit,
	elective bit,
	course_dept varchar(100),
	course_prereq varchar (90),
	PRIMARY KEY (course_number));
